{
  "name": "sidemath",
  "displayName": "sidemath",
  "description": "mathlive on your side",
  "version": "1.4.0",
  "repository": "https://github.com/dannypsnl/sidemath",
  "publisher": "dannypsnl",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sidemath.open",
        "title": "Sidemath: Open panel"
      },
      {
        "command": "sidemath.open_selection",
        "title": "Sidemath: Open selected formula in panel"
      }
    ],
    "keybindings": [
      {
        "command": "sidemath.open_selection",
        "key": "Alt+M",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0"
  },
  "dependencies": {
    "mathlive": "^0.107.0",
    "tex2typst": "^0.3.21"
  }
}
